<div class="uk-container">
<h4>Your Matches</h4>
  <% @matches.each do |match| %>
  <div>
    <div class="uk-child-width-1-2@s">
        <% flat = match.other_party(current_user.flat) %>
        <div class="uk-card uk-card-default uk-card-small uk-card-body">
          <h5 class="uk-card"><%= flat.title %></h5>
          <img class="uk-border-rounded" src=<%= flat.photo%> />
          <div class="uk-card-body uk-flex uk-padding-remove-vertical uk-margin-small-top">
          <p class="uk-margin-remove mr-2"><i class="fas fa-euro-sign mx-2"></i><%= flat.rent %></p>
          <p class="uk-margin-remove mr-2"><i class="fas fa-home mx-2"></i><%= flat.square_meter %></p>
          <p class="uk-margin-remove mr-2"><i class="fas fa-ticket-alt mx-2"></i><%= flat.vacant_from %></p>
          <p class= "uk-margin-remove mr-2"><i class="fas fa-door-open mx-2"></i><%= flat.bedroom + 1 %></p>
        </div>
          <%= link_to "Send a message", "#", class: "uk-button uk-button-danger uk-width-1-1 uk-margin-small-top" %>
          <%= link_to "More details about person", match_path(match), class: "uk-button uk-button-primary uk-width-1-1 uk-margin-small-top" %>
        </div>
  </div>
  </div>
  <% end %>
</div>
